{
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    }
  },
  "PathImagenes": "C:\\inetpub\\wwwroot\\SSF\\Principal\\ImagenesCargadas\\Incidencias\\",
  "PathRepoIncidencias": "c:\\repoIncidencias\\",
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [ "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/webapiSSF-.log",
          "rollingInterval": "Day",
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} {CorrelationId} {Level:u3}] {Username} {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  }
}